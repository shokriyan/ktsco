<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity"
	nodeOrientation="RIGHT_TO_LEFT" prefHeight="635.0" prefWidth="1166.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.ktsco.controllers.admin.UsersController">
	<children>
		<VBox prefHeight="635.0" prefWidth="1166.0">
			<children>
				<HBox prefHeight="100.0" prefWidth="1166.0" spacing="50.0">
					<padding>
						<Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
					</padding>
					<children>
						<Button fx:id="btnAddNewUser" mnemonicParsing="false"
							onAction="#allButtonsAction" prefHeight="27.0" prefWidth="150.0"
							text="ثبت کاربر جدید" />
						<Button fx:id="btnEditUser" layoutX="40.0" layoutY="40.0"
							mnemonicParsing="false" onAction="#allButtonsAction"
							prefHeight="27.0" prefWidth="150.0" text="ایجاد تغییرات" />
						<Button fx:id="btnDeleteUser" layoutX="40.0" layoutY="40.0"
							mnemonicParsing="false" onAction="#allButtonsAction"
							prefHeight="27.0" prefWidth="150.0" text="حذف کاربر" />
						<Region prefHeight="200.0" prefWidth="200.0"
							HBox.hgrow="ALWAYS" />
						<Button fx:id="btnReload" layoutX="340.0" layoutY="40.0"
							mnemonicParsing="false" onAction="#allButtonsAction"
							prefHeight="27.0" prefWidth="150.0" text="بارگذاری مجدد" />
					</children>
				</HBox>
				<TableView fx:id="userTable" prefHeight="535.0"
					prefWidth="200.0">
					<columns>
						<TableColumn fx:id="tcUserID" minWidth="250.0"
							prefWidth="59.0" resizable="false" text="شماره کاربری" />
						<TableColumn fx:id="tcUsername" minWidth="250.0"
							prefWidth="304.0" resizable="false" text="نام کاربری" />
						<TableColumn fx:id="tcPassword" minWidth="250.0"
							prefWidth="304.0" resizable="false" text="رمز ورود" />
						<TableColumn fx:id="tcAccess" minWidth="250.0"
							prefWidth="304.0" resizable="false" text="سطح دسترسی" />
					</columns>
				</TableView>
			</children>
		</VBox>
	</children>
</Pane>
