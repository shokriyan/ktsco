<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity"
	nodeOrientation="RIGHT_TO_LEFT" prefHeight="500.0" prefWidth="400.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.ktsco.controllers.admin.DeleteUsersController">
	<children>
		<HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0"
			styleClass="top-panel">
			<children>
				<Text fx:id="topViewLabel" strokeType="OUTSIDE"
					strokeWidth="0.0">
					<font>
						<Font size="18.0" />
					</font>
				</Text>
			</children>
		</HBox>
		<HBox prefHeight="400.0" prefWidth="200.0">
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
			<children>
				<VBox prefHeight="200.0" prefWidth="100.0" spacing="30.0">
					<children>
						<Label text="نام کاربری" />
						<Label layoutX="20.0" layoutY="20.0" text="رمز ورود" />
						<Label layoutX="20.0" layoutY="37.0" text="نوع دسترسی" />
					</children>
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
				</VBox>
				<VBox alignment="TOP_CENTER" prefHeight="200.0"
					prefWidth="230.0" spacing="20.0">
					<children>
						<ComboBox fx:id="comboUserName"
							onAction="#onUserComboValueChange" prefWidth="230.0" />
						<TextField fx:id="txtPassword" editable="false"
							layoutX="10.0" layoutY="10.0" />
						<TextField fx:id="txtAccessType" editable="false" />
						<Region prefHeight="200.0" prefWidth="200.0" />
						<Label fx:id="lblErrorMsg" textFill="RED" />
						<Button fx:id="btnSubmit" mnemonicParsing="false"
							onAction="#allButtonsAction" prefWidth="230.0" text="تایید" />
						<Button fx:id="btnCancel" mnemonicParsing="false"
							onAction="#allButtonsAction" prefWidth="230.0" text="خروج" />
					</children>
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
				</VBox>
			</children>
		</HBox>
	</children>
</VBox>
